  <joint name="leg_fl_roll_joint" type="revolute">
    <parent link="leg_fl_pitch"/>
    <child link="leg_fl_roll"/>
    <origin xyz="0 0 -0.1" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="0" velocity="0.5"/>
    <dynamics damping="0.7"/>
  </joint>

  <!-- Track -->
  <link name="track_fl">
    <visual>
      <geometry><cylinder radius="0.05" length="0.3"/></geometry>
      <origin xyz="0 0 0" rpy="1.57 0 0"/>
      <material name="black"><color rgba="0 0 0 1"/></material>
    </visual>
    <inertial>
      <mass value="2"/>
      <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.02"/>
    </inertial>
  </link>
  <joint name="leg_fl_track_joint" type="continuous">
    <parent link="leg_fl_roll"/>
    <child link="track_fl"/>
    <origin xyz="0 0 -0.1" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

