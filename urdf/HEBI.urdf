<?xml version="1.0"?>
<robot name="HEBI">

  <!-- World Frame -->
  <link name="world"/>
  <joint name="world_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
  </joint>

  <!-- Base Link -->
  <link name="base_link">
    <visual>
      <geometry><box size="1 0.6 0.2"/></geometry>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <material name="gray"><color rgba="0.5 0.5 0.5 1"/></material>
    </visual>
    <inertial>
      <mass value="10"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
  </link>

  <!-- FRONT LEFT LEG -->
  <!-- First segment of telescopic leg (fixed part) -->
  <link name="leg_fl_fixed">
    <visual>
      <geometry><cylinder radius="0.03" length="0.3"/></geometry>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
      <material name="blue"><color rgba="0 0 0.8 1"/></material>
    </visual>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.005"/>
    </inertial>
  </link>
  <joint name="leg_fl_fixed_joint" type="fixed">
    <parent link="base_link"/>
    <child link="leg_fl_fixed"/>
    <origin xyz="0.4 0.3 0.1" rpy="0 0 0"/> <!-- Z aggiustato per allineare base -->
  </joint>

  <!-- Second segment (telescopic part) -->
  <link name="leg_fl_extension">
    <visual>
      <geometry><cylinder radius="0.025" length="0.3"/></geometry>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
      <material name="red"><color rgba="0.8 0 0 1"/></material>
    </visual>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.005"/>
    </inertial>
  </link>
  <joint name="leg_fl_prismatic" type="prismatic">
    <parent link="leg_fl_fixed"/>
    <child link="leg_fl_extension"/>
    <origin xyz="0 0 0.15" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="0.3" effort="100" velocity="0.1"/>
  </joint>

  <!-- Pitch Joint -->
  <link name="leg_fl_pitch">
    <visual>
      <geometry><box size="0.05 0.05 0.05"/></geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <material name="dark_gray"><color rgba="0.3 0.3 0.3 1"/></material>
    </visual>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.005"/>
    </inertial>
  </link>
  <joint name="leg_fl_pitch_joint" type="revolute">
    <parent link="leg_fl_extension"/>
    <child link="leg_fl_pitch"/>
    <origin xyz="0 0 0.3" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1.0"/>
    <dynamics damping="0.7"/>
  </joint>

  <!-- Roll Joint -->
  <link name="leg_fl_roll">
    <visual>
      <geometry><box size="0.05 0.05 0.05"/></geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
